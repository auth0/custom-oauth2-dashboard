{
  "name": "custom-social-connections",
  "version": "1.1.10",
  "description": "This application gives Auth0 customers the possibility to manage custom social connections in an easy way.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-dev && node dist/custom-social-connections.js",
    "webpack": "webpack --config webpack.ui.config.js && webpack --config webpack.dev.config.js",
    "wt": "webpack --config webpack.ui.config.js && webpack --config webpack.wt.config.js",
    "clean": "npm install clean-css-cli -g && rm -rf dist && mkdir -p dist/public && cleancss -o dist/public/custom-social-connections-1.1.10.min.css src/public/css/styles.css",
    "build": "npm run clean && npm run wt",
    "build-dev": "npm run clean && npm run webpack"
  },
  "keywords": [
    "auth0",
    "extension",
    "oauth2"
  ],
  "author": "javier.centurion@auth0.com",
  "license": "MIT",
  "dependencies": {
    "auth0-oauth2-express": "1.3.0",
    "auth0-api-jwt-rsa-validation": "0.0.1",
    "body-parser": "1.18.2",
    "express": "4.14.0",
    "express-jwt": "5.3.1",
    "jade": "1.11.0",
    "webtask-tools": "3.2.0"
  },
  "devDependencies": {
    "classnames": "~2.1.3",
    "codemirror": "^5.8.0",
    "flux": "~2.0.1",
    "js-beautify": "^1.5.10",
    "jade-loader": "^0.8.0",
    "jsx-loader": "^0.13.2",
    "keymirror": "~0.1.0",
    "lodash": "^4.11.1",
    "object-assign": "2.0.0",
    "react": "~0.13.0",
    "react-codemirror": "~0.1.7",
    "request-promise": "^3.0.0",
    "string-replace-webpack-plugin": "0.0.3",
    "webpack": "^1.12.9"
  }
}
